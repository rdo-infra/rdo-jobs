parseable: true
quiet: false
exclude_paths:
    - playbooks/legacy
skip_list:
    - ANSIBLE0010  # Package installs should not use latest.
    - 601  # Due to https://github.com/ansible/ansible-lint/issues/470
    - 602  # Due to https://github.com/ansible/ansible-lint/issues/457
    - 106  # To allow role names with dash(-)
    - 208  # To allow tasks without explicit file modes
verbosity: 1
